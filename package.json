{
  "name": "clubpanel",
  "version": "1.0.0",
  "description": "ClubPanel is a modular open-source webpanel for managing any club.",
  "main": "bundle.js",
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "npx next build",
    "build:modules": "node build-modules.js",
    "build": "npm run build:modules && npm run build:server && npm run build:client",
    "start": "node ./dist/server/index.js",
    "buildstart": "npm run build && npm run start",
    "dev": "npm run build:modules && npm run build:server & node ./dist/server/index.js dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClubPanel/ClubPanel.git"
  },
  "author": "Jonah Uellenberg",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ClubPanel/ClubPanel/issues"
  },
  "homepage": "https://github.com/ClubPanel/ClubPanel#readme",
  "private": false,
  "dependencies": {
    "@chakra-ui/react": "^1.6.3",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "framer-motion": "^4",
    "fs-extra": "^10.0.0",
    "mongoose": "^5.12.13",
    "mongoose-sequence": "^5.3.1",
    "next": "^11.0.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-async": "^10.0.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-remove-scroll": "^2.4.2",
    "sass": "^1.34.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.11",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-sequence": "^3.0.5",
    "@types/node": "^15.6.2",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.24.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-node-externals": "^3.0.0"
  }
}

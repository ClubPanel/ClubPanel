{
  "name": "clubpanel",
  "version": "1.0.0",
  "description": "ClubPanel is a modular open-source webpanel for managing any club.",
  "main": "bundle.js",
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "npx next build",
    "build:modules": "node build-modules.js",
    "build:config": "node create-configs.js",
    "build": "npm run build:modules && npm run build:server && npm run build:config && npm run build:client",
    "start": "node ./dist/server/index.js",
    "buildstart": "npm run build && npm run start",
    "dev": "npm run build:modules dev && npm run build:server && npm run build:config && node ./dist/server/index.js dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClubPanel/ClubPanel.git"
  },
  "author": "Jonah Uellenberg",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ClubPanel/ClubPanel/issues"
  },
  "homepage": "https://github.com/ClubPanel/ClubPanel#readme",
  "private": false,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "framer-motion": "^4",
    "fs-extra": "^10.0.0",
    "mongoose": "^5.13.0",
    "mongoose-sequence": "^5.3.1",
    "next": "^12.1.5",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-async": "^10.0.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-remove-scroll": "^2.4.2",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.11",
    "@types/mongoose-sequence": "^3.0.5",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-react": "^7.24.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-node-externals": "^3.0.0"
  }
}
